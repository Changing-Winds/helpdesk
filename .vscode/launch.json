{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "helpdesk",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "helpdesk Web",
      "request": "launch",
      "type": "dart",
      "args": ["-d", "chrome","--web-port", "7357"]
    },
    {
      "name": "helpdesk HTML renderer",
      "request": "launch",
      "type": "dart",
      "args": ["-d", "chrome","--web-port", "7357", "--web-renderer", "html"]
    },
    {
      "name": "helpdesk (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "helpdesk (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}